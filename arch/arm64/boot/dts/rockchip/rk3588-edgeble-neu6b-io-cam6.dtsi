/ {
	dvdd_1v1_cam6: dvdd-1v1-cam6-regulator {
		compatible = "regulator-fixed";
		regulator-name = "dvdd_1v1_cam6";
		enable-active-high;
		gpio = <&gpio1 RK_PB2 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&pwdn_gpio_cam6>;
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <1100000>;
		regulator-max-microvolt = <1100000>;
		vin-supply = <&vcc5v0_sys>;
	};

	dovdd_1v8_cam6: dovdd-1v8-cam6-regulator {
		compatible = "regulator-fixed";
		regulator-name = "dovdd_1v8_cam6";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		vin-supply = <&dvdd_1v1_cam6>;
	};

	avdd_2v9_cam6: avdd-2v9-cam6-regulator {
		compatible = "regulator-fixed";
		regulator-name = "avdd_2v9_cam6";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <2900000>;
		regulator-max-microvolt = <2900000>;
		vin-supply = <&dovdd_1v8_cam6>;
	};
};

&pinctrl {
	cam {
		pwdn_gpio_cam6: pwdn-gpio-cam6 {
			rockchip,pins = <1 RK_PB2 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
};

&i2c5 {
	pinctrl-names = "default";
	pinctrl-0 = <&i2c5m4_xfer>;
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";

	imx415_6: imx415@36 {
		compatible = "sony,imx415";
		reg = <0x36>;
		clocks = <&cru CLK_MIPI_CAMARAOUT_M4>;
		clock-names = "xvclk";
		pinctrl-names = "default";
		pinctrl-0 = <&mipim0_camera4_clk>;
		power-domains = <&power RK3588_PD_VI>;
		reset-gpios = <&gpio0 RK_PA0 GPIO_ACTIVE_LOW>;
		avdd-supply = <&avdd_2v9_cam6>;
		dovdd-supply = <&dovdd_1v8_cam6>;
		dvdd-supply = <&dvdd_1v1_cam6>;
		rockchip,camera-module-index = <5>;
		rockchip,camera-module-facing = "back";
		rockchip,camera-module-name = "EDGEBLE-CAM4K-A00";
		rockchip,camera-module-lens-name = "DEFAULT";
		port {
			imx415_6_out0: endpoint {
				remote-endpoint = <&mipidphy5_in_ucam0>;
				data-lanes = <3 4>;
			};
		};
	};
};

&csi2_dphy1_hw {
	status = "okay";
};

&csi2_dphy5 {
	status = "okay";

	ports {
		#address-cells = <1>;
		#size-cells = <0>;

		port@0 {
			reg = <0>;
			#address-cells = <1>;
			#size-cells = <0>;

			mipidphy5_in_ucam0: endpoint@1 {
				reg = <1>;
				remote-endpoint = <&imx415_6_out0>;
				data-lanes = <3 4>;
			};
		};

		port@1 {
			reg = <1>;
			#address-cells = <1>;
			#size-cells = <0>;

			csidphy5_out: endpoint@0 {
				reg = <0>;
				remote-endpoint = <&mipi5_csi2_input>;
			};
		};
	};
};

&mipi5_csi2 {
	status = "okay";

	ports {
		#address-cells = <1>;
		#size-cells = <0>;

		port@0 {
			reg = <0>;
			#address-cells = <1>;
			#size-cells = <0>;

			mipi5_csi2_input: endpoint@1 {
				reg = <1>;
				remote-endpoint = <&csidphy5_out>;
			};
		};

		port@1 {
			reg = <1>;
			#address-cells = <1>;
			#size-cells = <0>;

			mipi5_csi2_output: endpoint@0 {
				reg = <0>;
				remote-endpoint = <&cif_mipi5_in0>;
			};
		};
	};
};

&rkcif {
	status = "okay";
};

&rkcif_mipi_lvds5 {
	status = "okay";

	port {
		cif_mipi5_in0: endpoint {
			remote-endpoint = <&mipi5_csi2_output>;
		};
	};
};

&rkcif_mipi_lvds5_sditf {
	status = "okay";

	port {
		mipi_lvds5_sditf: endpoint {
			remote-endpoint = <&isp1_vir2>;
		};
	};
};

&rkcif_mmu {
	status = "okay";
};

&rkisp1 {
	status = "okay";
};

&isp1_mmu {
	status = "okay";
};

&rkisp1_vir2 {
	status = "okay";

	port {
		#address-cells = <1>;
		#size-cells = <0>;

		isp1_vir2: endpoint@0 {
			reg = <0>;
			remote-endpoint = <&mipi_lvds5_sditf>;
		};
	};
};
